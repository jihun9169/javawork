package mart;
	//일반고객 이순신, id 10010, 포인트1000점
	//vip 김유신 아이디10020 포인트10000점
public class CustomerTest {
	
public static void main(String[] args) {
	 Customer customerlee = new Customer();
	 customerlee.customerName = "이순신";
	 customerlee.customerId = "10010";
	 customerlee.bonusPoint = 1000;
	 customerlee.customerAddress = "경기도 부천시";
	 System.out.println(customerlee.showCustomerInfo());
	 
	 
	 VIPCustomer vipcustomerkim = new VIPCustomer();
	 vipcustomerkim.customerName = "김유신";
	 vipcustomerkim.customerId = "10020";
	 vipcustomerkim.bonusPoint = 10000;
	 vipcustomerkim.customerAddress = "경기도 부천시";
	 System.out.println(vipcustomerkim.showPeopleInfo());
	
}
}
